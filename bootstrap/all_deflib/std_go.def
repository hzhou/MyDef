subcode: basic_frame
    func main() {
        $call main
    }

#---------------------------------------- 
subcode: check_err
    $if err!=nil
        panic(err)

#---------------------------------------- 
subcode: open_r(f)
    _in, err := os.Open($(f))
    $call check_err
    _input := bufio.NewScanner(_in)
    $while _input.Scan()
        $(set:line=_input.Text())
        BLOCK
    _in.Close()

subcode: open_w(f)
    _out, err := os.Create($(f))
    $call check_err
    BLOCK
    _out.Close()




include: c/string.def

page: test, basic_frame
    subcode: main
	$call test_string

subcode: test_string
    $my string b: "Hello World!"
    $print $b

    $my string a
    $my tn_a=123456
    $my tf_b=123.456
    a.$clear
    a.$add "a = ", tn_a, ", b = ", tf_b
    $print $a

    a.$clear
    a.$add b
    $print $a

    a.$fmt " a=$tn_a, b=$tf_b"
    $print $a

subcode:: parsecode_func_other
    $elif $func eq "fmt"
        my ($n, $fmt)=fmt_string($param)
        MyDef::compileutil::set_current_macro("fmt_n", $n)
        MyDef::compileutil::set_current_macro("fmt", $fmt)
        return
    $elif $func eq "print"
        $global $print_type=1, $print_newline=1
        $if !$param
            push @$out, "puts(\"\");"
        $else
            $param=~s/^\s+//
            $if $param=~/usesub:\s*(\w+)/
                $print_type=$1
            $elif $param=~/newline:\s*(\w+)/
                $if $1 eq '0'
                    $print_newline=0
                $else
                    $print_newline=1
            $else
                #--- fmt_string, add newline
                my ($n, $fmt)=fmt_string($param, $print_newline)
                $if $print_type==1
                    $if $n==0 and $fmt=~/^"(.*)\\n"/
                        push @$out, "puts(\"$1\");"
                    $elif $fmt=~/^"%s\\n", (.*)/
                        push @$out, "puts($1);"
                    $else
                        push @$out, "printf($fmt);"
                $elif $print_type
                    MyDef::compileutil::call_sub("$print_type, $fmt")
        return


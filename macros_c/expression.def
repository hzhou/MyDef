subcode:: check_expression_init
    my @stack
    my @types

subcode: exp_push(exp, type)
    push @stack, $(exp)
    push @types, $(type)

subcode: exp_pop(assign)
    $(if:assign=undef)
	pop @stack
	pop @types
    $(else)
	my $(assign)=pop @stack
	pop @types


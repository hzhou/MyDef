page: mydef_page
    output_dir: script
    subcode: main
	#Usage: perl createpages.pl def_file
	use MyDef;
	my $def_file=$ARGV[0]
	$if !-f $def_file
	    die "Please supply data definition file.";
	my $module_type;
	my %config;
	$foreach my $a in @ARGV
	    $if $a =~ /-m(\w+)/
		$config{module} = $1
	    $elif $a =~/-o(\S+)/
		$config{output_dir} = $1

	MyDef::init(%config);
	MyDef::import_data($ARGV[0]);

	$pages=$MyDef::def->{pages};

	$while my ($t, $p)=each (%$pages)
	    $if $p->{subpage}
		next
	    $if $p->{module} 
		$if $p->{module} ne $MyDef::var->{module}
		next
	    MyDef::createpage($t);

subcode: dump_array(a)
    print "Dump array $(a)\n"
    $foreach my $t in @$(a)
        print "    ", $t, "\n"

subcode: dump_line(name)
    my $yellow="\033[33;1m";
    my $normal="\033[0m";
    print "$yellow $(name): [$l]$normal\n"

subcode: dump_def
    print "Dump def $def\n"
    $while my ($k, $v) = each %$def
        print "    ", "$k: $v\n"
        $if $k eq "codes"
            $while my ($k2, $v2) = each %$v
                print "        ", $v2->{type}, "code: $k2\n"

